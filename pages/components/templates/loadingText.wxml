<template name="loadingText">
  <view wx:if="{{isLoading}}" class="weui-loadmore">
    <view class="weui-loading"></view>
    <view class="weui-loadmore__tips">Loading</view>
  </view>
  <view wx:if="{{isNoMoreData}}" class="weui-loadmore weui-loadmore_line">
    <view class="weui-loadmore__tips weui-loadmore__tips_in-line">No More Data</view>
  </view>
</template>
